<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
     

        <record id="action_report_sale_ksa_company" model="ir.actions.report">
            <field name="name">Entool Sale Order </field>
            <field name="model">sale.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">entool_company_custom_report.prosys_swag_sale_orde_report</field>
            <field name="report_file">entool_company_custom_report.prosys_swag_sale_orde_report</field>
            <field name="binding_model_id" ref="sale.model_sale_order"/>
            <field name="binding_type">report</field>

        </record>


  

        <record id="paperformat_action_report_tax_invoice_cus" model="report.paperformat">
            <field name="name">entool VAT Invoice</field>
			<field name="default" eval="True" />
			<field name="format">custom</field>
			<field name="page_height">0</field>
			<field name="page_width">0</field>
			<field name="orientation">Portrait</field>
			<field name="margin_top">125</field>
			<field name="margin_bottom">50</field>
			<field name="margin_left">5</field>
			<field name="margin_right">5</field>
			<field name="header_line" eval="False" />
			<field name="header_spacing">120</field>
			<field name="dpi">120</field>
        </record>

       

        <record id="action_report_swage_invoice" model="ir.actions.report">
            <field name="name">Entool Invoice Template</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">entool_company_custom_report.report_swage_custom_invoice</field>
            <field name="report_file">entool_company_custom_report.report_swage_custom_invoice</field>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="entool_company_custom_report.paperformat_action_report_tax_invoice_cus"/>
        </record>




  


        <record id="action_en_tool_china_company" model="ir.actions.report">
            <field name="name">Entool Stock recept</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">entool_company_custom_report.delevry_slip_china_customer_custom_pdf</field>
            <field name="report_file">entool_company_custom_report.delevry_slip_china_customer_custom_pdf</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        
        
        </record>



       



    </data> 

</odoo>
