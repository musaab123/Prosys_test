<odoo>
    <data>

        <record id="product_district_district_action_custom" model="ir.actions.act_window">
            <field name="name">District</field>
            <field name="res_model">district.district</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="product_district_district_view" model="ir.ui.view">
            <field name="name">District</field>
            <field name="model">district.district</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group name="name" string="District">
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="product_district_district_view_test" model="ir.ui.view">
            <field name="name">District</field>
            <field name="model">district.district</field>
            <field name="arch" type="xml">
                <tree>
                   
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

     

        <record id="district_district_action_custom" model="ir.actions.act_window">
            <field name="name">District</field>
            <field name="res_model">district.district</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="customer_district_district_custom"
                  name="District"
                  action="district_district_action_custom"
                  parent="contacts.menu_localisation"
                  sequence="5"/>
   
              
    </data>
</odoo>